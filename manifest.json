{
    "manifest_version": 3,
    "name": "Bionic Enhance Reader",
    "version": "0.0.2",
    "description": "Speed Up Your Reading By 10 Folds",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128":"img/icon128.png"
    },
    "permissions": ["activeTab","scripting"],
    "action": {
        "default_popup": "html/Popup.html",
        "default_title": "Click to Access Advanced Features"
    }
}