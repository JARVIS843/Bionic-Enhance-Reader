{
    "manifest_version": 3,
    "name": "Bionic Enhance Reader",
    "version": "0.0.1",
    "description": "Assist your reading with Bionic Enhance Reader",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128":"img/icon128.png"
    },
    "action": {
        "default_popup": "html/content.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/content.js"]
        }
    ]
}